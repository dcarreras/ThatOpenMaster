{
    "compilerOptions": {
        "target": "ES2020",
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
        "module": "ESNext",
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "strict": true,
        "isolatedModules": true,
        "noEmit": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": ["src"]
}
